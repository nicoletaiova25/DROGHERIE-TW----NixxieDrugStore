    <p>
        <label>
            Username: <input type="text" name="username" id="inp-username" value="nixxer<%= Math.floor(Math.random()*100000) %>" required pattern="[A-Za-z0-9#_./]+">
        </label>
    </p>
    <p>
        <label>
            Nume: <input type="text" name="nume" id="inp-nume" value="NixxieUser" required>
        </label>
    </p>
    <p>
        <label>
            Prenume: <input type="text" name="prenume" id="inp-prenume" value="Nixxie" required>
        </label>
    </p>
    <p>
        <label>
            Parola: <input id="parl" type="password" name="parola" required>
        </label>
    </p>
    <p id="p-reintrod">
        <label>
            Reintroduceti parola: <input id="rparl" type="password" name="rparola" required>
        </label>
    </p>
    <p>
        <label>
            Email: <input type="email" name="email" id="inp-email" required value="nixxieUser@gmail.com" size="50">
        </label>
    </p>
    <p>
        <label>
            Data na»ôterii: <input type="date" name="data_nasterii" id="inp-data-nasterii" required>
        </label>
    </p>
    <p>
        <label>
            Telefon: <input type="tel" name="telefon" id="inp-telefon">
        </label>
    </p>
    <p>
        <label>
            Culoare text chat: <input type="color" name="culoare_chat" id="inp-culoare-chat" value="#000000">
        </label>
    </p>
    <p>
        <label>
            Fotografie: <input type="file" name="poza">
        </label>
    </p>
    <p>
        <input type="submit" value="Trimite">
        <input type="reset" value="Reseteaza">
    </p>
</form>
